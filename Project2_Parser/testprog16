PROGRAM circle;
	(*Using Unassigned Variable*)
VAR
	(*Variable declarations*)
	r, a, p, b : REAL; 
	i, j : INTEGER;	
BEGIN
	r := 8;
	p := 0;
	a := 0;
	
	j := -1;
	IF ( r > 5) THEN
	  a := (3.14) + k * r;
	IF (a < 100) THEN
	    p := 2 * 3.14 * b; 
	(*Display the results*)
	WRITELN  ('The output results are ' , r, p, b);
